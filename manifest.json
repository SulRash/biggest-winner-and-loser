{
    "manifest_version": 2,
    "name": "Stock Market Winners and Losers",
    "version": "1.0",
    "description": "Displays the biggest winner and loser in the stock market for the current trading day.",
    "permissions": ["activeTab", "tabs", "webRequest"],
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "48": "icons/icon48.png"
      }
    },
    "icons": {
      "48": "icons/icon48.png"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content-script.js"]
      }
    ]
}
  