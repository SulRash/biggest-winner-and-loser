{
    "manifest_version": 2,
    "name": "Stock Market Gainers and Losers",
    "version": "1.0",
    "description": "Displays the biggest winner and loser in the stock market for the current trading day.",
    "permissions": ["activeTab", "webRequest"],
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "icons": {
      "48": "icon.png"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
  